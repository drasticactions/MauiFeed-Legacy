<Application
    x:Class="MauiFeed.WinUI.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MauiFeed">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <!-- Other merged dictionaries here -->
            </ResourceDictionary.MergedDictionaries>
            
            <local:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
            <local:BooleanToVisibilityInverter x:Key="BooleanToVisibilityInverter"/>
            <local:NullToVisibilityConverter x:Key="NullToVisibilityConverter"/>
            <local:FavoriteLabelConverter x:Key="FavoriteLabelConverter"/>
            <local:UnreadReadLabelConverter x:Key="UnreadReadLabelConverter"/>
            <local:HtmlToTextConverter x:Key="HtmlToTextConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
